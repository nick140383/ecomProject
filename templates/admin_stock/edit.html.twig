{% extends 'admin/base.html.twig' %}

{% block title %}stock edition!{% endblock %}

{% block body %}
    <div class="container">
        <h1>Update stock </h1>
        {{ form_start(form) }}
        {{ form_row(form.quantite) }}
        {{ form_row(form.modeleChaussure, {'disabled' : true }) }}
        {{ form_row(form.taille, {'disabled': true}) }}
        <button type="submit" class="btn btn-primary">edit stock</button>
        {{ form_end(form) }}

    </div>

{% endblock %}